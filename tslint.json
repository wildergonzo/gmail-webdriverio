{
    "defaultSeverity": "error",
    "extends": "tslint:recommended",
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "no-var-requires": true,
        "no-console": false,
        "quotemark": [true, "single", "avoid-escape"],
        "indent": [true, "spaces", 4],
        "no-var-keyword": true,
        "whitespace": false
    },
    "jsRules": {
        "max-line-length": {
            "options": [120]
        }
    }
}